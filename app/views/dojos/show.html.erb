<form class="" action="index.html" method="post">
<h1 style="color:Green">Dojo Branch</h1>
<h2 style="color:darkred"> <%= @dojo.branch %> </h2>
<h3><%= @dojo.street %></h3>
<h4><%= @dojo.city %>, <%= @dojo.state %></h4>
<br>
<hr>
<h2 style="color:Green">Student Roster</h2>
<table border='1'>
  <thead>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
    <th> Dojo</th>
    <th>Actions</th>
  </thead>
    <% @dojo.students.each do |student| %>
    <tr>
      <td><%= student.first_name %></td>
      <td><%= student.last_name %></td>
      <td><%= student.email %></td>
      <td><%= @dojo.branch %></td>
      <td>
        <a href="/dojos/<%= @dojo.id %>/students/<%= student.id %>">Show</a> |
        <a href="/dojos/<%= @dojo.id %>/students/<%= student.id %>/edit">Edit</a> |
        <a href="/dojos/<%= @dojo.id %>/students/<%= student.id %>" data-method="delete">Delete</a>
      </td>
    </tr>
    <% end %>
</table>
<hr>
<a href="/"><button type="button">Go Back</button></a>
<a href="/dojos/<%= @dojo.id %>/students/new"><button type="button">Add Student</button></a>
</form>
