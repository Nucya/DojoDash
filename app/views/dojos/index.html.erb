<table border="1">
  <thead>
    <tr>
      <th>Branches</th>
      <th>Street</th>
      <th>City</th>
      <th>State</th>
      <th>Actions</th>
    </tr>
  </thead>
  <% @dojos.each do |dojo| %>
    <tr>
      <td> <%= dojo.branch %> </td>
      <td> <%= dojo.street %> </td>
      <td> <%= dojo.city %> </td>
      <td> <%= dojo.state %> </td>
      <td>
        <a href="/dojos/<%= dojo.id %>">Show</a> |
        <a href="/dojos/<%=dojo.id%>/edit">Edit</a> |
        <a href="/dojos/<%= dojo.id %>" data-method="delete">Delete</a>
      </td>
    </tr>
  <% end %>
</table>
<br>
<a href="/dojos/new"> <button type="button">Create Dojo</button></a>
