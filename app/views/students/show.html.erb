<h2 style="color:Green">Student Roster</h2>
<h5>Email <%= @student.email %> </h5>
<table border='1'>
  <thead>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
    <th> Dojo</th>
  </thead>
    <% @students.each do |student| %>
    <tr>
      <td><%= student.first_name %></td>
      <td><%= student.last_name %></td>
      <td><%= student.email %></td>
      <td><%= @dojos.branch %></td>
    </tr>
    <% end %>
</table>
<hr>
<a href="/dojos/<%= @dojos.id %>">Go Back</a>
